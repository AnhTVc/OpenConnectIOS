auth-user-pass
ping 5
ping-restart 30
dev tun
pull
# MTU
tun-mtu 1500
tun-mtu-extra 32
mssfix 1450
resolv-retry infinite
nobind
persist-key
verb 3
key-direction 1
# Routing
route-delay 0 120
ns-cert-type server
hand-window 180
route-metric 1
topology subnet
# Ciphers
#comp-lzo adaptive # LZO Compression

tls-client
cipher AES-128-CBC

<connection>
remote be-s01.hspa.network
</connection>

